{
  // Use IntelliSense to learn about possible attributes.
  // Hover to view descriptions of existing attributes.
  // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Debug polyn",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/cmd/polyn",
      "cwd": "${workspaceFolder}",
      "args": ["install", "20.17.0"]
    },
    {
      "name": "Debug setup",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/install/cmd/setup"
    },
    {
      "name": "Debug uninstall",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/uninstall"
    },
    {
      "name": "Debug serve",
      "type": "go",
      "request": "launch",
      "mode": "debug",
      "program": "${workspaceFolder}/web/cmd/serve"
    },
    {
      "name": "Debug gui",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:4200",
      "webRoot": "${workspaceFolder}/web/gui",
      "preLaunchTask": "start"
    }
  ]
}
